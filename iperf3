h2 pkill iperf3
h3 pkill iperf3

h2 iperf3 -s -p 5004 &
h1 iperf3 -c h2 -p 5004 -u -b 8M | tee ./results/iperf5004.txt &

h2 iperf3 -s -p 5201 &
h2 iperf3 -s -p 5202 &
h1 iperf3 -c h2 -p 5201 -u -b 8M | tee ./results/iperf5201.txt &
h3 iperf3 -c h2 -p 5202 -u -b 8M | tee ./results/iperf5202.txt &

